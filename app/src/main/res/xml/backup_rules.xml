<full-backup-content>
    <!-- Inclure les préférences de l'app pour sauvegarde -->
    <include domain="sharedpref" path="CallBlockerPrefs.xml"/>

    <!-- Exclure les fichiers temporaires -->
    <exclude domain="file" path="temp/"/>
